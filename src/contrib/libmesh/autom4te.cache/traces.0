                 [
                   LIBS="-lelan $LIBS"
                   MPI_LIBS="-lelan $MPI_LIBS"
                 ],
                 [AC_MSG_ERROR([Could not find elan library... exiting])])
  fi

  AC_CHECK_LIB([mpi],
               [MPI_Init],
               [
                 MPI_LIBS="-lmpi $MPI_LIBS"
                 MPI_LIBS_PATHS="-L$MPI_LIBS_PATH"
                 MPI_IMPL="mpi"
                 AC_MSG_RESULT([Found valid MPI installlaion...])
