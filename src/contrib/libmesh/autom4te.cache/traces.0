    dnl action-if-found
    [
      ac_cv_cxx_dlopen=yes
    ],
    dnl action-if-not-found
    [
      ac_cv_cxx_dlopen=no
    ])

  dnl If AC_SEARCH_LIBS worked, try to compile a test code
  if (test "$ac_cv_cxx_dlopen" = yes); then
    AC_MSG_CHECKING([whether the c++ compiler supports dlopen/dlsym/dlclose])
