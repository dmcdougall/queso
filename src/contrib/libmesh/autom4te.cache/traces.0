    [
      AC_MSG_RESULT(yes)
      ac_cv_cxx_dlopen=yes
    ],
    dnl action-if-false
    [
      AC_MSG_RESULT(no)
      ac_cv_cxx_dlopen=no
    ])

    AC_LANG_POP([C++])
  fi
])
