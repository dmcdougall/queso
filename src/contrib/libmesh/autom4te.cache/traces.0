  # Ensure the comiler finds the header file...
  if test -e $MPI_INCLUDES_PATH/mpi.h; then
    echo "note: using $MPI_INCLUDES_PATH/mpi.h"
    tmpCPPFLAGS=$CPPFLAGS
    AC_LANG_SAVE
    AC_LANG_CPLUSPLUS
    CPPFLAGS="-I$MPI_INCLUDES_PATH $CPPFLAGS"
    AC_CHECK_HEADER([mpi.h],
